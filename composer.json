{
    "name": "rndrmu/statamic-metatags",
    "description": "An addon for Statamic 3 to manage and customize any meta tag.",
    "autoload": {
        "psr-4": {
          "rndrmu\\StatamicMetatags\\": "src"
        }
    },
    "authors": [
        {
            "name": "rei",
            "email": "hi@rndmru.moe"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Meta tags",
            "description": "An addon for Statamic 3 to manage and customize any meta tag."
        },
        "laravel": {
            "providers": [
              "rndrmu\\StatamicMetatags\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "statamic/cms": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false
        }
    }
}
